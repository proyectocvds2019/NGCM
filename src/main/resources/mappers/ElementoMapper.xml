<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="edu.eci.cvds.sampleprj.dao.mybatis.mappers.ElementoMapper">

	<resultMap type = 'Elementos' id = 'ElementosResult'>
		<id property = 'id' column = 'id'/>
		<result property = 'tipo' column = 'tipo'/>
		<result property = 'idEquipo' column = 'idEquipo'/>
		<result property = 'registradoPor' column = 'registradoPor'/>
	</resultMap>
	
	<insert parameterType = "map" id = "registrarElemento">	
		INSERT INTO elementos (id, tipo, registradoPor)
		VALUES (#{el.id},#{el.tipo},#{u.correo});
	</insert>
	
</mapper>